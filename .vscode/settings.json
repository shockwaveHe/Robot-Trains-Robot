{
    "[python]": {
        "editor.defaultFormatter": "charliermarsh.ruff",
        "editor.formatOnSave": true,
        "editor.codeActionsOnSave": {
            "source.organizeImports": "explicit"
        },
    },
    "C_Cpp.errorSquiggles": "disabled",
    "files.associations": {
        "istream": "cpp",
        "__locale": "cpp",
        "locale": "cpp",
        "cstddef": "cpp",
        "ostream": "cpp",
        "complex": "cpp",
        "chrono": "cpp"
    }
}