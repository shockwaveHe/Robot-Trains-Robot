    <spatial name="torso_right_hind" limited="true" range="0 0.25" width="0.003" stiffness="500" damping="5">
      <site site="weld_site"/>
      <site site="torso_right_hind_site"/> <!-- The cube is attached to the attachment site -->
    </spatial>
  </tendon>
